<div class='text-center' style='margin-bottom:50px'>

  <h1>Sign In</h1>
  <br>
  <%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>
  <br>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br>

  <div>
    <%= submit_tag "Log In", class:'btn btn-primary' %>
  </div>
  <% end %>
</div>
